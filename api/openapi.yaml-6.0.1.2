            allow:
              type: boolean
          required:
            - allow
        All_User_Access:
          type: object
          properties:
            level:
              type: string
              enum: [AgentAdmin]
          required:
            - level
        User_Access:
          type: object
          x-patternProperties:
            (*):
              description: user
              type: object
              properties:
                level:
                  type: string
                  enum: [AgentAdmin]
              required:
                - level
        Agent_Server_Public_Certificate:
          type: string
        Agent_Server_Private_Key:
          type: string
        Agent_Server_Private_Key_Password:
          type: string
        Only_From_Client_Public_Certificates:
          type: object
          x-patternProperties:
            (*):
              description: clientname
              type: string
      required:
        - Agent_Server_Public_Certificate
    Alert:
      type: object
      properties:
        Notification_Type:
          type: string
          enum: [EMAIL, SNS, SNMP, SLACK]
        Email_Recipients:
          type: array
