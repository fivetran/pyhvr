                finish_after_cycle:
                  type: boolean
                expiry_date:
                  type: string
                  format: date-time
                recv_expiry:
                  type: string
                  format: date-time
      responses:
        '200':
          description: OK
          content:
            application/json:
              schema:
                type: object
                properties:
                  ctrl_id:
                    type: string
                required:
                  - ctrl_id
        '400':
          description: Bad Request
          content:
            text/plain; charset=utf-8:
              schema:
                type: string
