paths:
  /v0/hubs/{hub}/activate:
    get:
      description: Check replication activation status of channels <br> ReadOnly <br> access level on hub
      tags:
        - Activate, Refresh, and Compare Interface
      parameters:
        - name: hub
          in: path
          required: true
          schema:
            type: string
        - name: query parameter prototype
          in: query
          schema:
            type: object
            properties:
              channels:
                type: array
                minItems: 1
