                    change_tables:
                      type: object
                      properties:
                        lossy_reactivate:
                          type: boolean
                        activating:
                          type: string
                          pattern: ^([a-zA-Z_])([0-9_a-zA-Z]*)
                        deactivating:
                          type: string
