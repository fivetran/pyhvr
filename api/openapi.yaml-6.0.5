                            type: boolean
                            enum: [true]
                          activating:
                            type: string
                            pattern: ^([a-zA-Z_])([0-9_a-zA-Z]*)
                          deactivating:
                            type: string
                            pattern: ^([a-zA-Z_])([0-9_a-zA-Z]*)
                        required:
