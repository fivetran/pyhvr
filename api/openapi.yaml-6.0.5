                        type: object
                        properties:
                          lossy_reactivate:
                            type: boolean
                            enum: [true]
                          activating:
                            type: string
                            pattern: ^([a-zA-Z_])([0-9_a-zA-Z]*)
                          deactivating:
                            type: string
                            pattern: ^([a-zA-Z_])([0-9_a-zA-Z]*)
                        required:
                          - lossy_reactivate
                      db_procs:
                        type: object
                        properties:
                          lossy_reactivate:
                            type: boolean
                          activating:
                            type: string
                            pattern: ^([a-zA-Z_])([0-9_a-zA-Z]*)
                          deactivating:
                            type: string
                            pattern: ^([a-zA-Z_])([0-9_a-zA-Z]*)
                        required:
                          - lossy_reactivate
                      supplemental_logging:
                        type: object
                        properties:
                          lossy_reactivate:
                            type: boolean
                            enum: [false]
                          activating:
                            type: string
                            pattern: ^([a-zA-Z_])([0-9_a-zA-Z]*)
                          deactivating:
                            type: string
                            pattern: ^([a-zA-Z_])([0-9_a-zA-Z]*)
                        required:
                          - lossy_reactivate
                      state_files:
                        type: object
                        properties:
                          lossy_reactivate:
                            type: boolean
                          activating:
                            type: string
                            pattern: ^([a-zA-Z_])([0-9_a-zA-Z]*)
                          deactivating:
                            type: string
                            pattern: ^([a-zA-Z_])([0-9_a-zA-Z]*)
                        required:
